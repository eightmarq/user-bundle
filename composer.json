{
    "name": "eightmarq/user-bundle",
    "description": "EightMarq user bundle - This bundle provides a basic user entity and user repository.",
    "type": "symfony-bundle",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Schvoy",
            "email": "norbert.schvoy@eightmarq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "EightMarq\\UserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "EightMarq\\UserBundle\\Tests\\": "tests/" }
    },
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/orm-pack": "*",
        "eightmarq/core-bundle": "^0.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
